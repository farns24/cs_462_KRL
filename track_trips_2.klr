ruleset track_trips {
  meta {
    name "Track Trips Part 2"
    author "Michael Farnsworth"
    description <<tracktrips>>
    logging on
    shares hello
  }
  
  global {
    hello = function(obj) {
      msg = "Hello " + obj;
      msg
    }
  }
  
 rule process_trip {
  select when car new_trip mileage re#(.*)# setting(m);
  send_directive("trip") with
    length = m
  }
 }
